      <main>

         <!-- sign up area start -->
         <section class="signup__area po-rel-z1 pt-100 pb-145">
            <div class="sign__shape">
               <img class="man-1" src="/images/icon/sign/man-3.png" alt="">
               <img class="man-2 man-22" src="/images/icon/sign/man-2.png" alt="">
               <img class="circle" src="/images/icon/sign/circle.png" alt="">
               <img class="zigzag" src="/images/icon/sign/zigzag.png" alt="">
               <img class="dot" src="/images/icon/sign/dot.png" alt="">
               <img class="bg" src="/images/icon/sign/sign-up.png" alt="">
               <img class="flower" src="/images/icon/sign/flower.png" alt="">
            </div>
            <div class="container">
               <div class="row">
                  <div class="col-xxl-8 offset-xxl-2 col-xl-8 offset-xl-2">
                     <div class="section__title-wrapper text-center mb-55">
                        <h2 class="section__title">Create a free <br>  Account</h2>
                  
                     </div>
                  </div>
               </div>
               <div class="row">
                  <div class="col-xxl-6 offset-xxl-3 col-xl-6 offset-xl-3 col-lg-8 offset-lg-2">
                     <div class="sign__wrapper white-bg">
                        <div class="sign__header mb-35">
                           <div class="sign__in text-center">
                              <a href="#" class="sign__social g-plus text-start mb-15"><i class="fab fa-google-plus-g"></i>Sign Up with Google</a>
                              <p> <span>........</span> Or, <a href="sign-up.html">sign up</a> with your email<span> ........</span> </p>
                           </div>
                        </div>
                        <div class="sign__form">
                           <form action="/signup" method="POST" enctype="multipart/form-data">
                              <div class="sign__input-wrapper mb-25">
                                 <h5>Account Type</h5>
                                 <div class="sign__input">
                                    <select name="type" id="user" onchange="newform()">
                                       <option name="student" id="student" value="student">Student</option>
                                       <option name="instructor" id="instructor" value="instructor">Instructor</option>
                                    </select>
                                    <i class="fal fa-user"></i>
                                 </div>
                                 
                              </div>
                              <div class="sign__input-wrapper mb-25">
                                 <h5>Full Name</h5>
                                 <div class="sign__input">
                                    <input type="text" name="name" id="name" value="<%= typeof name != 'undefined' ? name : '' %>" placeholder="Full name">
                                    <i class="fal fa-user"></i>
                                 </div>
                              </div>
                              <div class="sign__input-wrapper mb-25">
                                 <h5>Work email</h5>
                                 <div class="sign__input">
                                    <input type="text" name="email" id="email" value="<%= typeof email != 'undefined' ? email : '' %>" placeholder="E-mail Address">
                                    <i class="fal fa-envelope"></i>
                                 </div>
                              </div>

                                 <div class="sign__input-wrapper mb-25">
                                    <h5>Class/Specialty</h5>
                                    <div class="sign__input">
                                       <input type="text" name="class" id="class" placeholder="Class/Specialty">
                                       <i class="fal fa-user"></i>
                                    </div>
                                 </div>
                                 <div class="sign__input-wrapper mb-25">
                                    <h5>Password</h5>
                                    <div class="sign__input">
                                       <input type="password" name="pass" id="pass" placeholder="Password">
                                       <i class="fal fa-lock"></i>
                                    </div>
                                 </div>
                                 <div class="sign__input-wrapper mb-10">
                                    <h5>Re-Password</h5>
                                    <div class="sign__input">
                                       <input type="password" name="re_pass" id="re_pass" placeholder="Re-Password">
                                       <i class="fal fa-lock"></i>
                                    </div>
                                 </div>

                                 <div class="sign__input-wrapper mb-10">
                                    <h5>Profile Picture</h5>
                                    <div class="sign__input">
                                       <input class="form-control" runat="Server" type="file" name="uploaded_image" enctype="multipart/form-data" accept="">
                                       <i class="fas fa-camera"></i>
                                    </div>
                                 </div>


                              <div class="sign__action d-flex justify-content-between mb-30">
                                 <div class="sign__agree d-flex align-items-center">
                                    <input class="m-check-input" type="checkbox" name="agree" id="agree">
                                    <label class="m-check-label" for="agree">I agree to the <a href="#">Terms & Conditions</a>
                                       </label>
                                 </div>
                              </div>
                              <button class="e-btn w-100"> <span></span> Sign Up</button>
                              <div class="sign__new text-center mt-20">
                                 <p>Already in Shikho ? <a href="/signin"> Sign In</a></p>
                              </div>
                           </form>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </section>
         <!-- sign up area end -->
         
      </main>

         <!-- footer area start -->
         <footer>
            <div class="footer__area footer-bg">
               <div class="footer__top pt-90 pb-40">
                  <div class="container">
                     <div class="row">
                        <div class="col-xxl-3 col-xl-3 col-lg-3 col-md-4 col-sm-6">
                           <div class="footer__widget mb-50">
                              <div class="footer__widget-head mb-22">
                                 <div class="footer__logo">
                                    <a href="index.html">
